apiVersion: v1
clusters:
- cluster:
    certificate-authority: ca.crt
    server: https://{{ groups['k8s-master'][0] }}:{{ k8s_api_secure_port }}
  name: {{ env_name }}
contexts:
- context:
    cluster: {{ env_name }}
    user: admin
  name: tap-context
current-context: tap-context
kind: Config
preferences: {}
users:
- name: admin
  user:
    client-certificate: admin.crt
    client-key: admin.key
